Verify that the disjunction enum style works

-- cue --
E1: "e1str1" | "e1str2" | "e1str3" @cuetsy(kind="enum")
E2: "e2str1" | "e2str2" | "e2str3" | "e2str4" @cuetsy(kind="enum")
E3: "e1str1" | "e1str2" | "e1str3" @cuetsy(targetType="enum")

-- ts  --
export enum E1 {
  E1str1 = 'e1str1',
  E1str2 = 'e1str2',
  E1str3 = 'e1str3',
}
export enum E2 {
  E2str1 = 'e2str1',
  E2str2 = 'e2str2',
  E2str3 = 'e2str3',
  E2str4 = 'e2str4',
}
export enum E3 {
  E1str1 = 'e1str1',
  E1str2 = 'e1str2',
  E1str3 = 'e1str3',
}